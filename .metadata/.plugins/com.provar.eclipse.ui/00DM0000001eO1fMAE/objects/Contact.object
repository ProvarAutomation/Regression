    <validationRules>
        <fullName>contact_email_cannot_be_company_email</fullName>
        <active>true</active>
        <description>if the contact&apos;s email address contains one of the company email address domains, and it&apos;s not named AutoTrader or COE Dealer for Testing, and the account record type is not Third-Party or Partner, user cannot save the contact record</description>
        <errorConditionFormula>AND( 

AND( 
 Account.RecordTypeId &lt;&gt; &apos;012E00000005ynY&apos;, 
 Account.RecordTypeId &lt;&gt; &apos;012E000000057U2&apos;),

AND(
Account.Name &lt;&gt; &quot;AutoTrader&quot;, 
Account.Name &lt;&gt; &quot;COE Dealer for Testing&quot;  
),

OR( 
CONTAINS(Email,&apos;@aisrebates.com&apos;), 
CONTAINS(Email,&apos;@auctiongenius.com&apos;), 
CONTAINS(Email,&apos;@auctiongenius.net&apos;), 
CONTAINS(Email,&apos;@autotrader.com&apos;), 
CONTAINS(Email,&apos;@autotradergroup.com&apos;), 
CONTAINS(Email,&apos;@autotradergroup.mail.onmicrosoft.com&apos;), 
CONTAINS(Email,&apos;@buybooktech.com&apos;), 
CONTAINS(Email,&apos;@cdmdata.com&apos;), 
CONTAINS(Email,&apos;@coxautoinc.com&apos;), 
CONTAINS(Email,&apos;@dealsonwheels.com&apos;), 
CONTAINS(Email,&apos;@haystak.com&apos;), 
CONTAINS(Email,&apos;@homenetauto.com&apos;), 
CONTAINS(Email,&apos;@innovativedealertechnologies.com&apos;), 
CONTAINS(Email,&apos;@kbb.com&apos;), 
CONTAINS(Email,&apos;@makemydeal.com&apos;), 
CONTAINS(Email,&apos;@quickvalues.com&apos;), 
CONTAINS(Email,&apos;@vauto.com&apos;), 
CONTAINS(Email,&apos;@vinsolutions.com&apos;), 
CONTAINS(Email,&apos;@manheim.com&apos;), 
CONTAINS(Email,&apos;@manheimcloud.onmicrosoft.com&apos;)
)
)</errorConditionFormula>
        <errorDisplayField>Email</errorDisplayField>
        <errorMessage>Unable to save contact. Cox Automotive employees cannot be added as contacts in Salesforce.</errorMessage>
    </validationRules>
